{
    "name": "@lenskit/react",
    "version": "0.1.0",
    "description": "A set of completely unstyled, fully accessible React components for LensKit",
    "main": "dist/index.cjs",
    "typings": "dist/index.d.ts",
    "module": "dist/lenskit.esm.js",
    "license": "MIT",
    "files": [
        "README.md",
        "dist"
    ],
    "exports": {
        "import": "./dist/lenskit.esm.js",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
    },
    "type": "module",
    "sideEffects": false,
    "engines": {
        "node": ">=10"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/daoleno/lenskit.git",
        "directory": "packages/@lenskit-react"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "prepublishOnly": "npm run build",
        "build": "../../scripts/build.sh --external:react --external:react-dom",
        "watch": "../../scripts/watch.sh --external:react --external:react-dom",
        "clean": "rimraf ./dist"
    },
    "peerDependencies": {
        "react": "^16 || ^17 || ^18",
        "react-dom": "^16 || ^17 || ^18"
    },
    "devDependencies": {
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/uuid": "^8.3.4",
        "esbuild": "^0.15.12",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "dependencies": {
        "@apollo/client": "^3.7.0",
        "ethers": "^5.7.1",
        "ipfs-http-client": "^58.0.1",
        "uuid": "^9.0.0",
        "wagmi": "^0.7.5"
    }
}